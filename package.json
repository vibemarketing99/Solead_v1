{
  "name": "solead",
  "version": "1.0.0",
  "description": "Autonomous lead discovery system for Threads using Stagehand AI automation",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "worker": "ts-node src/workers/index.ts",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:coverage": "jest --coverage",
    "test:threads": "npx tsx demos/test-threads-integration.ts",
    "test:enhanced": "npx tsx demos/test-enhanced-system.ts",
    "test:live": "npx tsx demos/live-lead-discovery-test.ts",
    "test:stagehand": "npx tsx plugins/stagehand/examples/simple-demo.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "db:migrate": "typeorm migration:run -d src/database/config.ts",
    "db:generate": "typeorm migration:generate -d src/database/config.ts",
    "db:seed": "ts-node scripts/seed.ts",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "threads",
    "automation",
    "stagehand",
    "lead-generation",
    "social-media"
  ],
  "author": "Solead Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vibemarketing99/Solead_v1.git"
  },
  "bugs": {
    "url": "https://github.com/vibemarketing99/Solead_v1/issues"
  },
  "homepage": "https://github.com/vibemarketing99/Solead_v1#readme",
  "dependencies": {
    "@browserbasehq/stagehand": "latest",
    "@google-cloud/kms": "^4.0.1",
    "aws-sdk": "^2.1500.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "natural": "^6.10.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "playwright": "^1.40.0",
    "prom-client": "^15.0.0",
    "redis": "^4.6.10",
    "sharp": "^0.33.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/natural": "^5.1.5",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.2",
    "typescript": "^5.3.2"
  }
}
